    &lt;div class=&quot;field-items&quot;&gt;
            &lt;div class=&quot;field-item odd&quot;&gt;
                    &lt;img  class=&quot;imagefield imagefield-field_exh_image&quot; width=&quot;219&quot; height=&quot;260&quot; title=&quot;In Memoriam, Chartres Cathedral, 1939&quot; alt=&quot;John Taylor Arms: &quot; src=&quot;http://zimmerli.rutgers.edu/sites/zimmerli/files/img/E_Arms_Chartres_D.jpg?1340113145&quot; /&gt;        &lt;/div&gt;
        &lt;/div&gt;
&lt;/div&gt;
&lt;div class=&quot;field field-type-text field-field-exh-img-desc&quot;&gt;
      &lt;div class=&quot;field-label&quot;&gt;Image Description:&amp;nbsp;&lt;/div&gt;
    &lt;div class=&quot;field-items&quot;&gt;
            &lt;div class=&quot;field-item odd&quot;&gt;
